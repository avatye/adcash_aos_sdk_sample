<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adcash_aos_sdk_sample"
        android:name=".App"
        tools:targetApi="31">

        <!-- region # ads-mediation -->
        <meta-data
            android:name="com.naver.gfpsdk.PUBLISHER_CD"
            android:value="YOUR_NAVER_CODE" />

        <meta-data
            android:name="com.mobon.sdk.MediaCode"
            android:value="YOUR_mobon_code" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="YOUR_gms_ads_APPLICATION_ID_code" />

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BannerActivity" android:screenOrientation="portrait"/>
        <activity android:name=".InterstitialActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>